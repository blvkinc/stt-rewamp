# Builder.io Configuration
# Copy this file to .env and add your actual values

# Builder.io Public API Key
# Get this from your Builder.io dashboard: https://builder.io/account/organization
VITE_PUBLIC_BUILDER_KEY=your_builder_io_public_key_here

# Development Environment
NODE_ENV=development

# Optional: Custom API endpoints
# VITE_API_URL=https://your-api-endpoint.com

# Optional: Analytics or tracking IDs
# VITE_GA_ID=your_google_analytics_id
# VITE_HOTJAR_ID=your_hotjar_id